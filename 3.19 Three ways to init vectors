#include <iostream>
#include <vector>
#include <string>

using namespace std;

int main()
{
	vector<int> s1(10, 42);
	vector<int> s2;
	vector<int> s3{42, 42, 42, 42, 42, 42, 42, 42, 42, 42};

	for (int i = 0; i < 10; ++i) {
		s2.push_back(42);
	}

	for (auto s: s1)
		cout << s << " ";

	cout << endl;
	for (auto x : s2)
		cout << x << " ";

	cout << endl;
	for (auto y : s3)
		cout << y << " ";
}
